
@{
    ViewBag.Title = "ApprovedRegistration";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var Account = (TCCB.Models.DAO.Account)Session[CommonConstants.USER_SESSION];
}


<div class="col-sm-10 col-sm-offset-1">
    <div class="checkIDBox">
        <h1 class="text-center text-success" style="font-size: 100px;"><span class="glyphicon glyphicon-ok"></span></h1>
        <h3 class="text-center text-success">Cập nhật thành công</h3>
        <h3 class="text-center text-danger">@Account.Code.Trim()@Account.ManagementUnit.Code@ViewBag.RegistrationInterviewId</h3>\
        <p class="text-center checkIDBox"><a href="/raxoathoso" role="button" class="btn btn-danger">Quay lai</a></p>
    </div>
</div>